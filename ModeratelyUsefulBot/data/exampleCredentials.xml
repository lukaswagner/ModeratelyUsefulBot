<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<credentials>
    <bots>
        <!-- Moderately Useful Bot -->
        <bot>
            <token>BOTTOKEN</token>
        </bot>
    </bots>
    <spotify>
        <auth>
            <clientId>CLIENTID</clientId>
            <clientSecret>CLIENTSECRET</clientSecret>
            <!-- generate this using SpotifyTokenGenerator -->
            <refreshToken>REFRESHTOKEN</refreshToken>
        </auth>
    </spotify>
    <reddit>
        <auth>
            <username>USERNAME</username>
            <password>PASSWORD</password>
            <clientId>CLIENTID</clientId>
            <clientSecret>CLIENTSECRET</clientSecret>
            <!-- you can leave this unchanged, it's not used, but required -->
            <redirectUri>http://www.example.com/unused/redirect/uri</redirectUri>
        </auth>
    </reddit>
</credentials>
