<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<config xmlns="config">
    <bots>
        <!-- Yet Another Test Bot - in this example, this is a feature bot -->
        <bot>
            <name>YATB</name>
            <autostart>true</autostart>
            <commands>
                <command>
                    <name>meme</name>
                    <action>RedditCommands.GetRandomMeme</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>playlist</name>
                    <action>SpotifyCommands.SendPlaylistStats</action>
                    <adminOnly>false</adminOnly>
                    <parameters>
                        <parameter>
                            <name>playlistUser</name>
                            <type>string</type>
                            <value>1155295024</value>
                        </parameter>
                        <parameter>
                            <name>playlistId</name>
                            <type>string</type>
                            <value>4rn5S00IB3kTbgtO1GSzD8</value>
                        </parameter>
                    </parameters>
                </command>
                <command>
                    <name>mention</name>
                    <action>MiscCommands.Mention</action>
                    <adminOnly>false</adminOnly>
                    <parameters>
                        <parameter>
                            <name>userList</name>
                            <type>int</type>
                            <list>
                                <value>168587717</value>
                            </list>
                        </parameter>
                    </parameters>
                </command>
                <command>
                    <names>
                        <name>start</name>
                        <name>help</name>
                    </names>
                    <action>MiscCommands.Help</action>
                    <adminOnly>false</adminOnly>
                </command>
            </commands>
            <admins>
                <!-- edit this if you're running the bot, otherwise I'm your admin :^) -->
                <admin>1685877170</admin>
            </admins>
        </bot>
        <!-- Additional Test Bot - in this example, this is a management bot -->
        <bot>
            <name>AddTest</name>
            <autostart>true</autostart>
            <commands>
                <!-- basic commands -->
                <command>
                    <name>ping</name>
                    <action>MiscCommands.Ping</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>help</name>
                    <action>MiscCommands.Help</action>
                    <adminOnly>false</adminOnly>
                </command>
                <command>
                    <name>log</name>
                    <action>MiscCommands.GetLog</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>logalot</name>
                    <action>MiscCommands.LogALot</action>
                    <adminOnly>true</adminOnly>
                </command>
                <!-- management commands -->
                <command>
                    <name>exit</name>
                    <action>ManagementCommands.Exit</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>restart</name>
                    <action>ManagementCommands.Restart</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>botlist</name>
                    <action>ManagementCommands.BotList</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>start</name>
                    <action>ManagementCommands.Start</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>stop</name>
                    <action>ManagementCommands.Stop</action>
                    <adminOnly>true</adminOnly>
                </command>
                <command>
                    <name>adminonly</name>
                    <action>ManagementCommands.SetAdminOnly</action>
                    <adminOnly>true</adminOnly>
                </command>
            </commands>
            <admins>
                <!-- edit this if you're running the bot, otherwise I'm your admin :^) -->
                <admin>168587717</admin>
            </admins>
        </bot>
    </bots>
    <log>
        <consoleLevel>Info</consoleLevel>
        <fileLevel>Off</fileLevel>
        <logTimes>true</logTimes>
    </log>
</config>
